"\355, \341\353\353\334\356\357\365\355, \352\341\351 \n"
"\355\341 \363\342\336\363\357\365\355 \364\341 bots \363\364\341 \357\360\357"
"\337\341 \357\351 \367\361\336\363\364\345\362 \350\341 \354\360\357\361\357"
"\375\355 \355\341 \367\361\347\363\351\354\357\360\357\351\336\363\357\365"
"\355 \n"
"\363\364\341 \344\351\352\334 \364\357\365\362 \364\341 \352\341\355\334\353"
"\351\341.\n"
"    \n"
"\307 \345\355\364\357\353\336 #BOT ADD# \360\361\357\363\350\335\364\345\351 "
