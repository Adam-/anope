"\347 \353\337\363\364\341 \360\361\374\363\342\341\363\347\362 \364\357\365 "
"\370\345\365\344\371\355\375\354\357\365 \363\341\362.\n"
"\301\365\364\336 \345\337\355\341\351 \347 \353\337\363\364\341 \364\371\355 "
"\344\351\345\365\350\375\355\363\345\371\355 \360\357\365 \350\341 \341\355"
"\341\343\355\371\361\337\346\357\355\364\341\351\n"
"\341\365\364\374\354\341\364\341 \341\360\374 \364\357\355 %s \363\341\355 "
"\345\360\351\364\361\345\360\364\357\337 \343\351\341 \364\347 \367\361\336"
"\363\347\n"
"\364\357\365 \370\345\365\344\371\355\375\354\357\365. \301\355 \350\335\353"
"\345\364\345 \355\341 \367\361\347\363\351\354\357\360\357\351\336\363\345"
"\364\345 \364\357 \370\345\365\344\376\355\365\354\357\n"
"\341\360\374 \354\337\341 \344\351\341\366\357\361\345\364\351\352\336 \344"
"\351\345\375\350\365\355\363\347, \360\361\335\360\345\351 \355\341 \341\355"
"\341\343\355\371\361\351\363\364\345\337\364\345\n"
"\341\360\374 \364\357\355 %s \354\345 \364\347 \367\361\336\363\347 \364\347"
"\362 \345\355\364\357\353\336\362 #IDENTIFY#.\n"
"\n"
"\320\341\361\341\344\345\337\343\354\341\364\341:\n"
"\n"
"    #ACCESS ADD anyone@*.bepeg.com#\n"
"        \305\360\351\364\361\335\360\345\351 \360\361\374\363\342\341\363"
"\347 \363\364\357 \367\361\336\363\364\347 \354\345 ID #anyone#\n"
"        \341\360\374 \357\360\357\351\357\344\336\360\357\364\345 \365\360"
"\357\353\357\343\351\363\364\336 \364\347\362 \344\351\345\375\350\365\355"
