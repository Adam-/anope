"\301\355 \345\360\351\353\335\356\345\364\345 #ALL#, \350\341 \363\342\336"
"\363\345\364\345 \374\353\341 \364\341 \354\347\355\375\354\341\364\334 \363"
"\341\362.\n"
"\n"
"\320\341\361\334\344\345\351\343\354\341:\n"
"\n"
"   #DEL 1#\n"
"      \304\351\341\343\361\334\366\345\364\341\351 \364\357 \360\361\376\364"
"\357 \354\336\355\365\354\341.(\355\357\375\354\345\361\357 1).\n"
