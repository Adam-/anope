"\n"
"\304\351\341\352\374\360\364\345\351 \354\351\341 \"\345\351\352\357\355\351"
"\352\336\" \363\375\355\344\345\363\347 \354\345 \364\357 \370\345\365\344"
"\376\355\365\354\374 \363\341\362.\n"
"\"\305\351\352\357\355\351\352\336\" \363\375\355\344\345\363\347 \345\337"
"\355\341\351 \341\365\364\336 \360\357\365 \363\364\347\355 \360\361\341\343"
"\354\341\364\351\352\374\364\347\364\341 \344\345\355\n"
"\365\366\337\363\364\341\364\341\351, \341\353\353\334 \357 IRC server \343"
